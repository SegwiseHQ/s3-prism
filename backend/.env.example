# Server Configuration
PORT=3001
NODE_ENV=development

# AWS Configuration
AWS_REGION=us-east-1

# Optional: Override AWS credentials (if not using ~/.aws/credentials)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# Optional: If you want to restrict to specific buckets (comma-separated)
# ALLOWED_BUCKETS=bucket1,bucket2,bucket3

# CORS Configuration
CORS_ORIGIN=http://localhost:8080

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Session Configuration
SESSION_SECRET=your_random_session_secret

# Allowed email domain for login (leave empty to allow all domains)
ALLOWED_EMAIL_DOMAIN=yourdomain.com

# Frontend URL for redirect after login
FRONTEND_URL=http://localhost:8080
